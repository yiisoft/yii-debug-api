{
    "name": "yiisoft/yii-debug-api",
    "type": "library",
    "description": "Yii debug API",
    "keywords": [
        "yii-debug-api",
        "yiisoft",
        "yii",
        "debug",
        "api"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-debug-api/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-debug-api"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "psr/container": "1.0",
        "nyholm/psr7": "^1.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/data-response": "^3.0@dev",
        "yiisoft/http": "^1.0",
        "yiisoft/json": "^1.0",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/yii-debug": "^3.0@dev"
    },
    "require-dev": {
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "phpunit/phpunit": "^9.3",
        "phan/phan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Debug\\Api\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "web": "config/web.php",
            "routes": "config/routes.php",
            "providers-web": "config/providers-web.php"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt",
        "test": [
            "phpunit --colors --no-interaction"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
